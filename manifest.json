{
  "name": "SAVE URL",
  "version": "1.0",
  "description": "SAVE URLs!",
  "permissions": ["tabs", "storage", "downloads"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/img.png",
      "32": "images/img@2x.png",
      "48": "images/img@3x.png",
      "128": "images/img@8x.png"
    }
  },
  "icons": {
    "16": "images/img.png",
    "32": "images/img@2x.png",
    "48": "images/img@3x.png",
    "128": "images/img@8x.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2
}